{
    "canisters": {
        "outcalls": {
            "type": "custom",
            "main": "src/outcalls/index.ts",
            "candid": "src/outcalls/index.did",
            "candid_gen": "http",
            "build": "npx azle outcalls",
            "wasm": ".azle/outcalls/outcalls.wasm",
            "gzip": true,
            "assets": [["src/frontend/dist", "dist"]],
            "build_assets": "npm run build",
            "metadata": [
                {
                    "name": "candid:service",
                    "path": "src/outcalls/index.did"
                },
                {
                    "name": "cdk:name",
                    "content": "azle"
                }
            ]
        }
    }
}
