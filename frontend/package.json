{
  "name": "cipher_ai_vault_frontend",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "setup": "npm install && npm audit fix && dfx start --clean --background && dfx deploy && dfx stop && echo 'The frontend has been setup successfully!'",
    "deploy": "dfx deploy --network ic",
    "format:css": "prettier --write \"frontend/styles/Parent.css\"",
    "format:css:check": "prettier --check \"frontend/styles/Parent.css\""
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "@dfinity/agent": "^2.1.1",
    "@dfinity/assets": "2.1.1",
    "@dfinity/candid": "2.1.1",
    "@dfinity/principal": "2.1.1",
    "@huggingface/transformers": "^3.0.0-alpha.19",
    "@msgpack/msgpack": "^3.0.0-beta2",
    "@types/node": "22.5.2",
    "@xenova/transformers": "^2.17.2",
    "antd": "^5.21.1",
    "azle": "0.23.0",
    "ic-auth": "^0.9.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "^5.3.0",
    "react-query": "^3.39.3",
    "recharts": "^2.12.7",
    "uuid": "^10.0.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
